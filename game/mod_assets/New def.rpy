    import random
    import string

    def random_string_generator(str_size, allowed_chars):
        return ''.join(random.choice(allowed_chars) for x in range(str_size))

    class GlitchWord(renpy.Displayable):

        def __init__(self, length, delay, **kwargs):
            super(GlitchWord, self).__init__(**kwargs)

            self.length = length
            self.delay = delay
            self.count = 0
            self.currentText = renpy.text.text.Text(random_string_generator(self.length, string.ascii_letters))

        def render(self, width, height, st, at):

            if self.count == 0:
                self.currentText = renpy.text.text.Text(random_string_generator(self.length, string.ascii_letters)) 


            self.count = (self.count + 1) % self.delay

            width, height = self.currentText.size()

            # Create the render we will return.
            render = renpy.Render(width, height)
            
            # Blit (draw) the child's render to our render.
            render.blit(renpy.render(self.currentText, width, height, st ,at),(0,-3),False)
        
            renpy.redraw(self, 0)

            # Return the render.
            return render


    def glitch_ran_word(tag, argument):
        return [ ( renpy.TEXT_DISPLAYABLE, GlitchWord(12,5)) ]

    config.self_closing_custom_text_tags["glitch_ran"] = glitch_ran_word



