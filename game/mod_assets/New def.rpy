transform dramatic_appearance():
    # Start with an initial zoom and position offset, centered
    zoom 0.1
    xalign 0.5  # Center horizontally
    yalign 0.5  # Center vertically

    # First zoom-in and zoom-out
    easein 0.5 zoom 1.2
    pause 0.1
    easeout 0.5 zoom 1.0

    # Second zoom-in and zoom-out
    easein 0.5 zoom 1.2
    pause 0.1
    easeout 0.5 zoom 1.0

    # Third zoom-in and zoom-out
    easein 0.5 zoom 1.2
    pause 0.1
    easeout 0.5 zoom 1.0

    # Final reveal - smoothly zoom to full size, keeping the background centered
    ease 1.0 zoom 1.0


